# PostgreSQL 数据库配置 (连接到share网络中的postgres)
DB_POSTGRESDB_HOST=share-postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password

# Redis 配置 (连接到share网络中的redis)
QUEUE_BULL_REDIS_HOST=share-redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_PASSWORD=your_redis_password

# n8n 加密密钥 (重要：请生成一个强密钥，至少32字符)
N8N_ENCRYPTION_KEY=your_secure_encryption_key_32_chars_min

# 基础认证
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_secure_admin_password

# 网络配置
WEBHOOK_URL=https://your-domain.com/webhook
N8N_HOST=your-domain.com